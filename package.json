{
  "name": "chess-zobrist",
  "version": "0.1.0",
  "description": "A TypeScript library for hashing chess positions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "prepare": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "validate": "npm run lint && npm run test && npm run build",
    "docs": "typedoc --out docs src/index.ts"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,ts}": "oxlint"
  },
  "keywords": [
    "chess",
    "hash",
    "typescript",
    "zobrist",
    "fen"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/CorentinGS/chess-zobrist"
  },
  "bugs": {
    "url": "https://github.com/CorentinGS/chess-zobrist/issues"
  },
  "homepage": "https://github.com/CorentinGS/chess-zobrist#readme",
  "author": "CorentinGS",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/jest": "29.5.14",
    "@types/node": "22.8.5",
    "jest": "29.7.0",
    "oxlint": "0.10.3",
    "rimraf": "6.0.1",
    "ts-jest": "29.2.5",
    "typedoc": "^0.26.10",
    "typescript": "5.6.3"
  }
}
